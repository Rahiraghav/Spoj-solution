package nk;
import java.util.Scanner;
public class check2 {
	public static void main(String[] args) {
		Scanner obj = new Scanner(System.in);
		int t=obj.nextInt(); 
		while(t>0) {
			int sum=0;
		int m=obj.nextInt();
		 
		for(int i=1;i>0;i++) {
			if(Math.pow(5, i)<=m) {
				int n=m/(int)Math.pow(5, i);
				sum+=n;
				}
			else {
				break;
			}
		}
		
	System.out.println(sum);

		t--;}
	}
}
